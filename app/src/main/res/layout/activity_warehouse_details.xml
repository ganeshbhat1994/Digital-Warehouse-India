<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@color/common_signin_btn_light_text_pressed"

    tools:context="com.example.w.maps.DetailsWarehouse"
   >
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Warehouse name"
            android:layout_marginBottom="7dp"
            android:textColor="#2f6699"/>
        <EditText
            android:id="@+id/warname"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:layout_marginBottom="7dp"
            android:background="@drawable/bg_edittext"
            android:inputType="text"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Reg.no"
            android:layout_marginBottom="7dp"
            android:textColor="#2f6699"/>
        <EditText
            android:id="@+id/warregno"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:textColor="#000000"
            android:background="@drawable/bg_edittext"
            android:inputType="text"/>



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Reg. Date"

            android:textColor="#2f6699"/>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:textColor="#000000">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="          YYYY"
                    android:textColor="#000000"/>
                <EditText
                    android:id="@+id/regyear"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="7dp"
                    android:textColor="#000000"
                    android:maxLength="4"
                    android:layout_marginRight="5dp"

                    android:background="@drawable/bg_edittext"
                    android:inputType="number"/>
            </LinearLayout>



                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="35dp"
                        android:layout_height="wrap_content"
                        android:text="      MM"
                        android:textColor="#000000"/>
                    <EditText
                        android:id="@+id/regmonth"
                        android:layout_width="35dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="7dp"
                        android:textColor="#000000"
                        android:maxLength="2"
                        android:layout_marginRight="5dp"
                        android:background="@drawable/bg_edittext"
                        android:inputType="number"/>
                </LinearLayout>



                     <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                         android:orientation="vertical">
                        <TextView
                            android:layout_width="35dp"
                            android:layout_height="wrap_content"
                            android:text="      DD"
                            android:textColor="#000000"/>
                        <EditText
                            android:id="@+id/regdate"
                            android:layout_width="35dp"
                            android:layout_height="wrap_content"
                            android:textColor="#000000"
                            android:maxLength="2"
                            android:layout_marginBottom="7dp"
                            android:background="@drawable/bg_edittext"
                            android:inputType="number"/>
                    </LinearLayout>


        </LinearLayout>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Access Period(Reg.valid till)"
            android:textColor="#2f6699"
            android:layout_marginTop="7dp"/>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginRight="5dp">
                <TextView
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:text="         YYYY"
                    android:textColor="#000000"/>
                <EditText
                    android:id="@+id/vregyear"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="7dp"
                    android:textColor="#000000"
                    android:maxLength="4"
                    android:background="@drawable/bg_edittext"
                    android:inputType="number"/>
            </LinearLayout>



            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginRight="5dp">
                <TextView
                    android:layout_width="35dp"
                    android:layout_height="wrap_content"
                    android:text="      MM"
                    android:textColor="#000000"/>
                <EditText
                    android:id="@+id/vregmonth"
                    android:layout_width="35dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="7dp"
                    android:textColor="#000000"
                    android:maxLength="2"
                    android:layout_marginRight="5dp"
                    android:background="@drawable/bg_edittext"
                    android:inputType="number"/>
            </LinearLayout>



            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="7dp"
                android:orientation="vertical">
                <TextView
                    android:layout_width="35dp"
                    android:layout_height="wrap_content"
                    android:text="      DD"
                    android:textColor="#000000"/>
                <EditText
                    android:id="@+id/vregdate"
                    android:layout_width="35dp"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:maxLength="2"
                    android:background="@drawable/bg_edittext"
                    android:inputType="number"/>
            </LinearLayout>


        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Total Space (tons)"
            android:layout_marginBottom="7dp"
            android:textColor="#2f6699"/>
        <EditText
            android:id="@+id/warcapacity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:textColor="#000000"
            android:background="@drawable/bg_edittext"
            android:inputType="number"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Available Space (tons)"
            android:layout_marginBottom="7dp"
            android:textColor="#2f6699"/>
        <EditText
            android:id="@+id/availability"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_edittext"
            android:layout_marginBottom="15dp"
            android:textColor="#000000"
            android:inputType="number"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Rent(Rs/period)"
            android:layout_marginBottom="7dp"
            android:textColor="#2f6699"/>
        <EditText
            android:id="@+id/rent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_edittext"
            android:textColor="#000000"
            android:layout_marginBottom="15dp"
            android:inputType="number"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Type of warehouse"
            android:layout_marginBottom="7dp"
            android:textColor="#2f6699"/>

        <RadioGroup
            android:id="@+id/radiogroupcold"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <RadioButton
                android:id="@+id/radiocold"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="Cold"
                android:buttonTint="#000000"
                android:textColor="#2f6699"/>
            <RadioButton
                android:id="@+id/radionormal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="Normal"
                android:buttonTint="#2f6699"
                android:layout_marginBottom="7dp"
                android:textColor="#2f6699"/>
        </RadioGroup>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Type of warehouse"
            android:layout_marginBottom="7dp"
            android:textColor="#2f6699"/>
        <RadioGroup
            android:id="@+id/radiogroup"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


                <RadioButton
                    android:id="@+id/radiocwc"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Central Warehouse"
                    android:buttonTint="#2f6699"
                    android:layout_marginBottom="3dp"
                    android:textColor="#2f6699"
                    android:checked="false" />
                <RadioButton
                    android:id="@+id/radiosws"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="3dp"
                    android:buttonTint="#2f6699"
                    android:text="State Warehouse"

                    android:textColor="#2f6699"/>
                <RadioButton
                    android:id="@+id/radiopcs"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Private Registered Warehouse"
                    android:buttonTint="#2f6699"
                    android:layout_marginBottom="3dp"
                    android:textColor="#2f6699"/>
                <RadioButton
                    android:id="@+id/radiopvt"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Private Unregistered Warehouse"
                    android:buttonTint="#2f6699"
                    android:layout_marginBottom="7dp"
                    android:textColor="#2f6699"/>





        </RadioGroup>


        <Button
            android:id="@+id/bfacility"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Add storages"
            android:layout_marginBottom="15dp"
            android:textColor="#ffffff"
            android:background="@drawable/rounded_edittext"
            />
        <Button
            android:id="@+id/warsubmit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Submit"
            android:textColor="#ffffff"
            android:background="@drawable/rounded_edittext"
            />

    </LinearLayout>

</ScrollView>
